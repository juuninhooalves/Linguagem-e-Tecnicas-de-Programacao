#include <stdio.h>
#include <locale.h>

int main(void)
{

setlocale(LC_ALL, "Portuguese");
float d, c, r;

printf("Digite a quantia em dolar que deseja converter pra real: US$");
scanf("%f", &d);

printf("Digite a cotação do dolar: ");
scanf("%f", &c);

r = d * c;

printf("US$%.2f equivale a R$%.2f", d, r);


}